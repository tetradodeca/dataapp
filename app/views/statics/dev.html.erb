<% @dates.each do |day| %>
  <%= day.date %><br>
  <% day.records.each do |record| %>
  <%= record.zone %><br>
  <%= [record.time_start, record.time_end] %><br>
  <%= record.activity %><br><br>
  <% end %>
<% end %>



<% @feedpod_dates.each do |day| %>
